{
	"name": "pasteandformat",
	"displayName": "pasteandformat",
	"description": "Paste and format with one command",
	"version": "0.0.2",
	"publisher": "spoeken",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.pasteAndFormat"
	],
	"main": "./extension",
	"contributes": {
        "keybindings": [{
            "command": "extension.pasteAndFormat",
            "key": "ctrl+shift+v",
            "mac": "cmd+shift+v",
            "when": "editorTextFocus"
        }]
	},
    
	"devDependencies": {
		"vscode": "0.10.x"
	}
}